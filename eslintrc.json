
{
    "env": {
        "jest": true,
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        
        "no-restricted-syntax": [
            "error", "LabeledStatement", "WithStatement"
        ],
   
        "import/extensions": 0

    },
    "parser": "babel-eslint"
}